{
  "connector": {
    "name": "local",
    "options": {
      "pattern": [
        "**",
        "!.git/**",
        "!node_modules/**"
      ]
    }
  },
  "extends": [
    "development"
  ],
  "formatters": [
    "stylish"
  ],
  "hints": {
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ]
  }
}